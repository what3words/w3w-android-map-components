<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.what3words.maps.google_maps_compose_sample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.W3wandroidcomponentsmaps">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="${mapsApiKey}" />

        <activity
            android:name=".UsingMapWrapperActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.W3wandroidcomponentsmaps">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UsingMapFragmentActivity"
            android:exported="false" />
        <activity
            android:name=".UsingGoogleMapComposeLibraryActivity"
            android:exported="false" />
    </application>

</manifest>